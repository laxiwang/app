<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhyx.halfroom.dao.BannerDao">
    <select id="select_banner" resultType="com.jhyx.halfroom.bean.Banner">
        select *
        from `banner`
        order by `create_time` desc
    </select>
</mapper>