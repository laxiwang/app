<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhyx.halfroom.dao.ManualDao">
    <select id="select_manual_by_type" resultType="com.jhyx.halfroom.bean.Manual">
        select *
        from activity_manual
        where type = #{type}
    </select>
</mapper>