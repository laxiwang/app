<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhyx.halfroom.dao.AdvertisementDao">
    <select id="select_app_advertisement" resultType="com.jhyx.halfroom.bean.Advertisement">
        select *
        from `app_advertisement`
        where `status` = 0
    </select>
</mapper>