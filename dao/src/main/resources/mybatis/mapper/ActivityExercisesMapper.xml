<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jhyx.halfroom.dao.ActivityExercisesDao">
    <select id="select_activity_exercises_by_user_type" resultType="com.jhyx.halfroom.bean.ActivityExercises">
        select *
        from `activity_exercises`
        where `user_type` = #{userType}
    </select>
</mapper>